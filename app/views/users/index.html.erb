<div class="container">
  <div class="transitions-enabled" id="beers">
    <% @users.each do |user| %>
    <div class="box panel panel-default">
      <%= link_to (image_tag user.profilepic.url(:medium)), user_path(user) %>
      <div class="panel-body">
        <%= link_to user.username, user_path(user) %>
      </div>
    </div>
    <% end %>
    <%= will_paginate @user, renderer: BootstrapPagination::Rails %>
  </div>
</div>